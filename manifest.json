{
    "manifest_version": 2,
    "name": "Bruteforce",
    "description": "Forked from www_brute as a mission to make this much better than the original was ever made to be, and redesign it to match the general publics opinon.",
    "version": "1.0.3",
 
    "icons": {
        "16": "settings/icon.png",
        "32": "settings/icon.png",
        "48": "settings/icon.png",
        "128": "settings/icon.png"
    },

    "permissions": [
        "activeTab", "tabs", "*://*/*"
    ],

    "background" : {
        "page": "settings/background.html"
    },

    "content_scripts": [ {
        "matches": ["*://*/*"],
        "js": ["bruteforce.js"]
    } ],

    "browser_action": {
        "default_title": "Bruteforce",
        "default_icon": "settings/icon.png",
        "default_popup": "popup/popup.html"
    }
}
